# Step 5: Add time-based columns
df_clean['date'] = pd.to_datetime(df_clean['date'])
today = pd.Timestamp.now()
df_clean['days_away'] = (df_clean['date'] - today).dt.days
df_clean['months_away'] = df_clean['days_away'] / 30

# Step 6: Add PRIORITY TIER (NEW!)
def assign_priority_tier(days_away):
    """
    Assign priority tier based on time until concert
    """
    if days_away < 0:
        return 'PAST'
    elif days_away <= 7:
        return 'URGENT'  # Buy tickets NOW
    elif days_away <= 30:
        return 'HIGH'    # Research & decide soon
    elif days_away <= 90:
        return 'MEDIUM'  # Watchlist - monitor prices
    elif days_away <= 180:
        return 'LOW'     # Future planning
    else:
        return 'FUTURE'  # Long-term watchlist

df_clean['priority_tier'] = df_clean['days_away'].apply(assign_priority_tier)

# Step 7: Add ACTION RECOMMENDATION (NEW!)
def recommend_action(priority_tier, price_min):
    """
    Recommend what to do based on priority and price availability
    """
    if priority_tier == 'PAST':
        return 'âŒ Event passed'
    elif priority_tier == 'URGENT':
        return 'ğŸš¨ BUY NOW - Event in next 7 days!'
    elif priority_tier == 'HIGH':
        if pd.notna(price_min):
            return 'ğŸŸï¸ DECIDE SOON - Check prices & availability'
        else:
            return 'ğŸŸï¸ DECIDE SOON - Research pricing'
    elif priority_tier in ['MEDIUM', 'LOW']:
        return 'ğŸ‘€ WATCHLIST - Monitor price drops'
    else:
        return 'ğŸ“… FUTURE - Keep on radar'

df_clean['action'] = df_clean.apply(
    lambda row: recommend_action(row['priority_tier'], row['price_min']),
    axis=1
)

# Step 8: Add PRICE TRACKING FLAG (NEW!)
def needs_price_tracking(priority_tier, price_min):
    """
    Flag events that need active price monitoring
    """
    # Track prices for events 7-90 days away without price data
    if priority_tier in ['HIGH', 'MEDIUM'] and pd.isna(price_min):
        return True
    return False

df_clean['track_price'] = df_clean.apply(
    lambda row: needs_price_tracking(row['priority_tier'], row['price_min']),
    axis=1
)

