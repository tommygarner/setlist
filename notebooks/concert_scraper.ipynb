def search_concerts_for_artist(artist_name, api_key, city=CITY, state_code=STATE_CODE, radius=SEARCH_RADIUS):
    """
    Search for concerts by artist name using Ticketmaster Discovery API
    """
    base_url = "https://app.ticketmaster.com/discovery/v2/events.json"

    params = {
        'apikey': api_key,
        'keyword': artist_name,
        'classificationName': 'Music',
        'city': city,
        'stateCode': state_code,
        'countryCode': COUNTRY_CODE,
        'radius': radius,
        'unit': 'miles',
        'sort': 'date,asc'
    }

    try:
        response = requests.get(base_url, params=params)
        response.raise_for_status()
        data = response.json()

        if '_embedded' in data and 'events' in data['_embedded']:
            return data['_embedded']['events']
        else:
            return []
    except Exception as e:
        print(f"Error searching for {artist_name}: {str(e)}")
        return []

print("âœ… Concert search function defined!")

def extract_concert_info(event):
    """
    Extract relevant information from a Ticketmaster event
    """
    concert_info = {
        'artist': event.get('name', 'N/A'),
        'date': 'N/A',
        'time': 'N/A',
        'venue': 'N/A',
        'city': 'N/A',
        'state': 'N/A',
        'price_min': 'N/A',
        'price_max': 'N/A',
        'currency': 'USD',
        'ticket_link': 'N/A',
        'event_id': event.get('id', 'N/A')
    }

    # Extract date and time
    if 'dates' in event and 'start' in event['dates']:
        start = event['dates']['start']
        if 'localDate' in start:
            concert_info['date'] = start['localDate']
        if 'localTime' in start:
            concert_info['time'] = start['localTime']

    # Extract venue information - FIX: venues is a list!
    if '_embedded' in event and 'venues' in event['_embedded']:
        venues = event['_embedded']['venues']
        if venues and len(venues) > 0:
            venue = venues[0]  # Get first venue from list
            concert_info['venue'] = venue.get('name', 'N/A')
            if 'city' in venue:
                concert_info['city'] = venue['city'].get('name', 'N/A')
            if 'state' in venue:
                concert_info['state'] = venue['state'].get('stateCode', 'N/A')

    # Extract pricing information
    if 'priceRanges' in event:
        price_range = event['priceRanges'][0]
        concert_info['price_min'] = price_range.get('min', 'N/A')
        concert_info['price_max'] = price_range.get('max', 'N/A')
        concert_info['currency'] = price_range.get('currency', 'USD')

    # Extract ticket link
    if 'url' in event:
        concert_info['ticket_link'] = event['url']

    return concert_info

